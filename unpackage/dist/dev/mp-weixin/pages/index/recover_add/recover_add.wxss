.container {
  height: 100vh;
  padding-top: 1rpx;
}
.container .s_title {
  font-size: 32rpx;
  margin-left: 20rpx;
}
.container > view {
  background: #fff;
  padding-top: 40rpx;
  padding-left: 24rpx;
  margin: 20rpx 0;
}
.container .upload {
  padding: 58rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.container .upload > view {
  width: 315rpx;
  text-align: center;
  font-weight: bold;
}
.container .upload > view image {
  width: 315rpx;
  height: 202rpx;
  margin-bottom: 38rpx;
}
.container .btn_line {
  margin: 100rpx auto 90rpx;
}
.middle_prop .middle_text {
  width: 90vw;
  padding: 0;
  margin: 0;
}
.middle_prop .middle_text > view {
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column nowrap;
          flex-flow: column nowrap;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.middle_prop .middle_text .prop_title {
  height: 112rpx;
  border-bottom: 1rpx solid #f3f3f3;
}
.middle_prop .middle_text .prop_content {
  height: 45%;
}
.middle_prop .middle_text .prop_content .c_28_888 {
  max-width: 420rpx;
  font-weight: 200 !important;
}
.middle_prop .middle_text .prop_content > image {
  width: 300rpx;
  height: 244rpx;
}
.middle_prop .middle_text .prop_content > image,
.middle_prop .middle_text .prop_content text,
.middle_prop .middle_text .prop_content view {
  margin-top: 60rpx;
}
.middle_prop .middle_text .prop_bottom {
  height: 70rpx;
}
.middle_prop .middle_text .prop_bottom > view {
  margin: 60rpx;
}
.middle_prop .middle_text .prop_bottom > .btn_2 {
  font-weight: 200;
  padding: 10rpx 60rpx;
  -webkit-border-radius: 44rpx;
          border-radius: 44rpx;
}
.middle_prop .middle_text .prop_bottom > .btn_squre {
  -webkit-border-radius: 10rpx !important;
          border-radius: 10rpx !important;
}
.else {
  width: 670rpx;
  height: 368rpx;
  line-height: 368rpx;
  background: #f3f3f3;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  text-align: center;
  color: #999999;
  font-size: 32rpx;
  font-weight: bold;
  margin: 30vh auto;
}
.else > view {

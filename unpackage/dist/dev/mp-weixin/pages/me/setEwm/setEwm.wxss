.container {
  background-color: #2E8EF4;
  font-size: 32rpx;
  position: static;
  z-index: -1;
}
.container > view {
  margin: 0 auto;
  position: relative;
  overflow: hidden;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.container > view > view {
  position: relative;
}
.container > view > image {
  width: 700rpx;
  height: 688rpx;
  position: absolute;
  top: 0;
}
.container > view:first-child {
  width: 700rpx;
  height: 688rpx;
}
.container > view:first-child > view {
  width: 100%;
  text-align: center;
}
.container > view:first-child .title {
  background: #f3f3f3;
  color: #2E8EF4;
  height: 108rpx;
  line-height: 108rpx;
}
.container > view:first-child .title + view image {
  width: 320rpx;
  height: 320rpx;
  margin-top: 72rpx;
}
.container .btn {
  width: 464rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #ffffff;
  -webkit-border-radius: 44rpx;
          border-radius: 44rpx;
  text-align: center;
  color: #2E8EF4;
  margin-top: 100rpx;
}

